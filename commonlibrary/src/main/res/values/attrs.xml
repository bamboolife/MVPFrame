<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountDownCircleView">
        <attr name="circle_bg_color" format="color" />
        <attr name="circle_pb_color" format="color" />
        <attr name="circle_pb_width" format="dimension" />
        <attr name="circle_text_color" format="color" />
        <attr name="circle_text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MainNavigateTabBar">
        <attr name="navigateTabTextColor" format="color|reference"/>
        <attr name="navigateTabSelectedTextColor" format="color|reference"/>
        <attr name="containerId" format="integer|reference"/>
        <attr name="navigateTabTextSize" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="XEditText">
        <attr name="x_separator" format="string"/>
        <attr name="x_disableClear" format="boolean"/>
        <attr name="x_clearDrawable" format="reference"/>
        <attr name="x_showPwdDrawable" format="reference"/>
        <attr name="x_hidePwdDrawable" format="reference"/>
        <attr name="x_disableEmoji" format="boolean"/>
        <attr name="x_disableTogglePwdDrawable" format="boolean"/>
    </declare-styleable>
</resources>